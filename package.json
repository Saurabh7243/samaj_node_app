{
  "name": "Family - TypeScript",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "nodemon dist/src/index.js",
    "debug": "concurrently \"tsc -w\" \"nodemon --inspect dist/src/index.js\"",
    "build": "rimraf dist & npx tsc",
    "typeorm": "npx typeorm-ts-node-commonjs",
    "migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run -d ./src/db.ts",
    "schema:sync": "npm run typeorm schema:sync -- -d ./src/db.ts ",
    "migration:show": "npm run typeorm migration:show -- -d ./src/db.ts ",
    "migration:generate": "npm run typeorm migration:generate -- -d ./src/db.ts",
    "typeorm:create": "npm run typeorm migration:create "
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.18",
    "@types/node": "^20.11.17",
    "@types/xlsx": "^0.0.36",
    "joi-to-typescript": "^4.10.0",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@types/body-parser": "^1.19.3",
    "@types/express-fileupload": "^1.4.4",
    "@types/multer": "^1.4.11",
    "@types/nodemailer": "^6.4.14",
    "@types/passport": "^1.0.13",
    "@types/passport-jwt": "^3.0.10",
    "@types/qrcode": "^1.5.5",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "concurrently": "^8.2.1",
    "cors": "^2.8.5",
