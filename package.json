    "crypto-ts": "^1.0.2",
    "dotenv": "^16.4.4",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "express-fileupload": "^1.4.3",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.45",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.9",
    "npm": "^10.2.4",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
